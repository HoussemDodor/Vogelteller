@model IEnumerable<Models.Project>

@{
    ViewBag.Title = "Index";
}

<h2>Alle Projecten</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.map)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.speciesID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.map)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.speciesID)
        </td>
    </tr>
}

</table>
